{
  "name": "vector",
  "version": "0.5.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:pages": "ng build --prod --output-path docs --base-href=/vector/",
    "build:prod": "ng build --prod",
    "deploy": "npm run build:pages && cp docs/index.html docs/404.html && gh-pages -d docs",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "lint:watch": "nodemon --exec \"ng lint\" --ext ts --ignore node_modules/",
    "e2e": "ng e2e",
    "preversion": "ng lint && ng test -- --no-watch --no-progress",
    "postversion": "git push && git push --tags"
  },
  "private": true,
  "dependencies": {
    "@angular-eslint/builder": "^13.2.0",
    "@angular-eslint/eslint-plugin": "^13.2.0",
    "@angular-eslint/eslint-plugin-template": "^13.2.0",
    "@angular-eslint/schematics": "^12.7.0",
    "@angular-eslint/template-parser": "^13.2.0",
    "@angular/animations": "^12.2.17",
    "@angular/cdk": "^12.2.13",
    "@angular/common": "^12.2.17",
    "@angular/compiler": "^12.2.17",
    "@angular/core": "^12.2.17",
    "@angular/flex-layout": "^12.0.0-beta.35",
    "@angular/forms": "^12.2.17",
    "@angular/material": "^12.2.13",
    "@angular/material-moment-adapter": "^12.2.13",
    "@angular/platform-browser": "^12.2.17",
    "@angular/platform-browser-dynamic": "^12.2.17",
    "@angular/router": "^12.2.17",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "core-js": "^3.10.1",
    "eslint": "^8.12.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^38.1.6",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-unicorn": "^42.0.0",
    "lasp-base-app-snippets": "^4.0.0",
    "lasp-footer": "^1.10.8",
    "lasp-nav": "^1.10.9",
    "lasp-search": "^1.0.7",
    "moment": "^2.24.0",
    "ngx-markdown": "^12.1.0",
    "nodemon": "^2.0.15",
    "rxjs": "^6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.18",
